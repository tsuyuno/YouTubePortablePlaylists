{
    "name": "YouTubePortablePlaylists",
	"manifest_version": 2,
    "version": "0.0.2",
     "browser_action": {
	      "default_icon": "image/icon.png",
	      "default_title": "放送中の番組を確認する",
          "default_popup": "html/popup.html"
	 },
    "description": "参加中のコミュニティ/チャンネルの放送番組を一覧表示します．", 
    "icons": {
        "16": "image/icon.png", 
        "32": "image/icon.png", 
        "64": "image/icon.png"
    }, 
    "permissions": [
        "alarms",
        "notifications",
        "identity",
        "http://www.nicovideo.jp/",
        "https://secure.nicovideo.jp/",
        "http://live.nicovideo.jp/",
        "http://api.ce.nicovideo.jp/",
        "http://icon.nimg.jp/community/",
        "http://flapi.nicovideo.jp/api/",
        "https://apis.google.com/",
        "https://accounts.google.com/*",
        "https://www.googleapis.com/*"
    ],
    "background": {
	    "scripts": ["js/jquery-2.1.4.min.js", "js/newArrival.js", "js/comuHolder.js", "js/nicovideo.js", "js/background.js"]
	},
    "options_page": "html/options.html",
    "web_accessible_resources": [
        "image/icon.png"
    ],
    "key": "",
    "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}
