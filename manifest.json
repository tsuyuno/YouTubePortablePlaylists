{
    "name": "YouTube PortablePlay lists",
	"manifest_version": 2,
    "version": "0.0.3",
     "browser_action": {
	      "default_icon": "image/icon.png",
	      "default_title": "YouTube PortablePlay lists",
          "default_popup": "html/popup.html"
	 },
    "description": "参加中のコミュニティ/チャンネルの放送番組を一覧表示します．", 
    "icons": {
        "16": "image/icon.png", 
        "32": "image/icon.png", 
        "64": "image/icon.png"
    }, 
    "permissions": [
        "alarms",
        "notifications",
        "identity",
        "https://apis.google.com/",
        "https://accounts.google.com/*",
        "https://www.googleapis.com/*",
        "https://www.youtube.com/"
    ],
    "background": {
	    "scripts": ["js/jquery-2.1.4.min.js", "js/newArrival.js", "js/comuHolder.js", "js/nicovideo.js", "js/background.js"]
	},
    "options_page": "html/options.html",
    "web_accessible_resources": [
        "image/icon.png"
    ],
    "key": "",
    "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}
